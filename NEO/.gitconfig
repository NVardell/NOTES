[user]
	email = NVardell@GMail.com
	name = NV
	username = NVardell
[core]
	aditor=vi
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
	pager=delta
	excludesfile=~/.gitignore
[init]
	defaultBranch=main
[color]
	ui=auto
[color "branch"]
	current=yellow bold
	local=green bold
	remote=cyan bold
[color "status"]
	added=green bold
	changed=yellow bold
	untracked=red bold
[color "diff"]
	meta=yellow bold
	frag=magenta bold
	old=red bold
	new=green bold
	whitespace=red reverse
[safe]
	directory = I:/Repo/NOTE
